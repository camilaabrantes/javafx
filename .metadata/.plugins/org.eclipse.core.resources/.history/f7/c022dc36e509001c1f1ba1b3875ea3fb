package gui;

import java.net.URL;
import java.util.ArrayList;
import java.util.List;
import java.util.ResourceBundle;

import javafx.collections.ObservableList;
import javafx.fxml.FXML;
import javafx.fxml.Initializable;
import javafx.scene.control.ComboBox;
import model.entities.Person;

public class ViewController implements Initializable{

	@FXML
	private ComboBox<Person> comboBoxPerson; //caixa com opções para serem selecionadas pelo usuário
	
	private ObservableList<Person> obsList;
	
	@Override
	public void initialize(URL url, ResourceBundle rb) {
		List<Person> personsList = new ArrayList<Person>();
		personsList.add(new Person(1, "Maria", "gmail"));
		personsList.add(new Person(2, "Alex", "gmail"));
		personsList.add(new Person(3, "Joao", "gmail"));
		
		
	}

}
